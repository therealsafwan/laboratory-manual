        dec += int(dig) * 2**i\par
        i += 1\par
         return dec\par
def oct2Hex(val):\par
    rev=val[::-1]\par
    dec = 0\par
    i = 0\par
    for dig in rev:\par
        dec += int(dig) * 8**i\par
        i += 1\par
    list=[]\par
    while dec != 0:\par
        list.append(dec%16)\par
        dec = dec // 16\par
