class Circle(Shape):\par
    def __init__(self,radius):\par
        self.area = 0\par
        self.name = "Circle"\par
        self.radius = radius\par
        \par
    def calcArea(self):\par
        self.area = math.pi * self.radius * self.radius\par
    \par
