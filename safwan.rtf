    if len(lst) > 1:\par
        mid = len(lst) // 2\par
        left_half = lst[:mid]\par
        right_half = lst[mid:]\par
\par
        merge_sort(left_half)\par
        merge_sort(right_half)\par
\par
        i = j = k = 0\par
\par
